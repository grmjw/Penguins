<?xml version="1.0"?>
<port xmlns:xi="http://www.w3.org/2001/XInclude" name="ComObjectListPort" port_id="67"  version="4.0.0" port_based_only="true">
    <description> Provides a list of tracked objects. </description>
    <segment type="static" name="GenericPortHeader" size="24" id="0">
    </segment>
    <segment type="header" name="StaticPortHeader" size="16" id="1">
        <description> Static header of object list port </description>
        <element_list>
            <element>
                <name>CycleTime</name>
                <description> Cycle Time </description>
                <element_offset>0 </element_offset>
                <size>4</size>
                <data_type>SP_FLOAT</data_type>
                <data_unit>_s</data_unit>
            </element>
            <element>
                <name>NumberOfObjects</name>
                <description> Provides the number of objects reported in this cycle  </description>
                <element_offset>4</element_offset>
                <segment_counter_id>2</segment_counter_id>
                <size>2</size>
                <data_type>SP_UINT16</data_type>
            </element>
            <element>
                <name>TimeStampOfMeasurement</name>
                <description> Time stamp of the detections generation in the corresponding processing cycle. </description>
                <element_offset>8</element_offset>
                <size>8</size>
                <data_type>SP_UINT64</data_type>
            </element>
        </element_list>
    </segment>
    <segment type="dynamic" name="Object" size="44" id="2">
        <description> Represents a single object  </description>
        <number_of_segments_name>NumberOfObjects</number_of_segments_name>
        <number_of_segments_offset>4</number_of_segments_offset>
        <number_of_segments_size>2</number_of_segments_size>
        <element_list>
            <element>
                <name>xPos</name>
                <description> X component of the position </description>
                <element_offset>0</element_offset>
                <size>4</size>
                <data_type>SP_FLOAT</data_type>
                <data_unit>_m</data_unit>
            </element>
            <element>
                <name>yPos</name>
                <description> Y component of the position </description>
                <element_offset>4</element_offset>
                <size>4</size>
                <data_type>SP_FLOAT</data_type>
                <data_unit>_m</data_unit>
            </element>
            <element>
                <name>zPos</name>
                <description> Z component of the position </description>
                <element_offset>8</element_offset>
                <size>4</size>
                <data_type>SP_FLOAT</data_type>
                <data_unit>_m</data_unit>
            </element>
            <element>
                <name>SpeedAbsolute</name>
                <description> Absolute object speed </description>
                <element_offset>12</element_offset>
                <size>4</size>
                <data_type>SP_FLOAT</data_type>
                <data_unit>_m_per_s</data_unit>
            </element>
            <element>
                <name>Heading</name>
                <description> Heading of an object </description>
                <element_offset>16</element_offset>
                <size>4</size>
                <data_type>SP_FLOAT</data_type>
                <data_unit>_rad</data_unit>
            </element>
            <element>
                <name>Length</name>
                <description> Length of an object </description>
                <element_offset>20</element_offset>
                <size>4</size>
                <data_type>SP_FLOAT</data_type>
                <data_unit>_m</data_unit>
            </element>
            <element>
                <name>Quality</name>
                <description> Quality of an object </description>
                <element_offset>28</element_offset>
                <size>4</size>
                <data_type>SP_FLOAT</data_type>
            </element>
            <element>
                <name>Acceleration</name>
                <description> Acceleration of an object </description>
                <element_offset>32</element_offset>
                <size>4</size>
                <data_type>SP_FLOAT</data_type>
                <data_unit>_m_per_s_sq</data_unit>
            </element>
            <element>
                <name>ObjectID</name>
                <description> unique tag number for identification of the object  </description>
                <element_offset>36</element_offset>
                <size>2</size>
                <data_type>SP_UINT16</data_type>
            </element>
            <element>
                <name>Status</name>
                <description> 0 = Object, 2 = Guardrail  </description>
                <element_offset>43</element_offset>
                <size>1</size>
                <data_type>SP_UINT8</data_type>
            </element>
        </element_list>
    </segment>
</port>

